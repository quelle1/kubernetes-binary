ADD https://dl.k8s.io/v1.14.0/kubernetes-client-linux-ppc64le.tar.gz /root/
ADD https://dl.k8s.io/v1.14.0/kubernetes-client-darwin-amd64.tar.gz /root/
ADD https://dl.k8s.io/v1.14.0/kubernetes-server-linux-amd64.tar.gz /root/
ADD https://dl.k8s.io/v1.14.0/kubernetes-server-linux-ppc64le.tar.gz /root/
ADD https://dl.k8s.io/v1.14.0/kubernetes-node-linux-ppc64le.tar.gz /root/
ADD https://dl.k8s.io/v1.14.0/kubernetes-node-linux-amd64.tar.gz /root/